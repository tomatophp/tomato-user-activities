{
    "name": "tomatophp/tomato-user-activities",
    "description": "Advanced logger to save user activities from requests",
    "keywords": [
        "logger",
        "request logger",
        "tomatophp",
        "user log",
        "user activities"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "EngFadyMondy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0",
        "monolog/monolog": "^2.9|^3.3",
        "ext-json": "*"
    },
    "require-dev": {
        "laravel/framework": "^9.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "~8.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "TomatoPHP\\TomatoUserActivities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomatoPHP\\TomatoUserActivities\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoUserActivities\\TomatoUserActivitiesServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
